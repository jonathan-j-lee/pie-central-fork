{
  "name": "@pioneers/dawn",
  "version": "1.0.0",
  "description": "PiE Robotics System Frontend",
  "main": "build/main.js",
  "scripts": {
    "build": "webpack --mode production --progress",
    "package": "npm run build && electron-builder",
    "start": "nodemon --watch build/main.js --exec \"electron .\"",
    "watch": "webpack --mode development --progress --watch"
  },
  "author": "Pioneers in Engineering",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.46.0",
    "react": "^17.0.2",
    "react-ace": "^9.4.1",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "css-loader": "^5.2.6",
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.9",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2"
  },
  "build": {
    "appId": "edu.berkeley.pioneers.dawn",
    "asar": true,
    "files": [
      "build/**/*"
    ],
    "linux": {
      "category": "Development",
      "executableName": "dawn",
      "artifactName": "${productName}-${version}.${ext}"
    }
  }
}
